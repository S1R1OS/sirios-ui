{
  "extends": "@sirios/ts-config/react.json",
  "include": [
    "src",
    ".next/types/**/*.ts"
  ],
  "compilerOptions": {
    "allowJs": true,
    "noEmit": true,
    "incremental": true,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  }
}
